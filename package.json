{
  "name": "$package_name",
  "version": "0.0.0",
  "description": "$package_description",
  "repository": "$package_author/$package_name",
  "author": "$package_author_name <$package_author_email>",
  "license": "MIT",
  "files": ["index.js", "example.js"],
  "scripts": {
    "test": "ava && linterjs .",
    "dev": "ava --watch & linterjs . --watch",
    "start": "node -r @std/esm ./index.js",
    "doc": "documentation readme index.js --section=API"
  },
  "engines": { "node": ">8.8" },
  "keywords": [],
  "tonicExampleFilename": "example.js",
  "ava": {
    "require": ["@std/esm"]
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true
  }
}
